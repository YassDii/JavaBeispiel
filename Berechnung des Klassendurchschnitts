import java.util.Scanner;

public class Klassendurchschnitt {

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        
        int math, Physik, chemisch, T端rkisch, Geschichte, Musik;

        System.out.println("Gib deine Mathenote ein :");
        math = input.nextInt();

        System.out.println("Geben Sie Ihre Physiknotiz ein :");
        Physik = input.nextInt();

        System.out.println("Geben Sie Ihre Chemienote ein :");
        chemisch = input.nextInt();

        System.out.println("Geben Sie Ihre t端rkische Note ein :");
        T端rkisch = input.nextInt();

        System.out.println("Geben Sie Ihre Geschichtsunterrichtsnote ein :");
        Geschichte = input.nextInt();

        System.out.println("Geben Sie Ihre Musiknote ein :");
        Musik = input.nextInt();

        int Gesamt = math + Physik + chemisch + T端rkisch + Geschichte + Musik ;
        
        int Durchschnitt = Gesamt/6;
        
        System.out.println("Dein Durchschnitt :" + Durchschnitt);

        String str = (Durchschnitt>=60)? "du hast bestanden" : "du konntest nicht bestehen";
        System.out.println("Deine Situation : " + str);

     
    }
}
